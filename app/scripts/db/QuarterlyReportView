 SELECT s.pid,
    s.name,
    s.portfolio,
    s.address,
    s.city,
    s.state,
    s.zip,
    s.msa,
    s.asset_category,
    s.asset_class,
    s.units,
    s.square_feet,
    s.buildings,
    s.year_built,
    q.date,
    q.occupancy,
    q.quarterly_distribution_per,
    q.quarterly_distribution_abs,
    q.ytd_distribution_per,
    q.ytd_distribution_abs
   FROM property s
     JOIN quarterlyreportmetrics q ON s.pid = q.property_id
  WHERE s.status::text = 'ACTIVE'::text;